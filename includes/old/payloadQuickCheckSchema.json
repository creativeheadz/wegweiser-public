{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"properties": {
	  "data": {
		"type": "object",
		"properties": {
		  "device": {
			"type": "object",
			"properties": {
			  "deviceUuid": {
				"type": "string",
				"format": "uuid"
			  },
			  "groupUuid": {
				"type": "string",
				"format": "uuid"
			  },
			  "systemtime": {
				"type": "integer",
				"minimum": 0
			  }
			},
			"required": ["deviceUuid", "groupUuid", "systemtime"]
		  },
		  "system": {
			"type": "object",
			"properties": {
			  "devicePlatform": {
				"type": "string"
			  },
			  "systemName": {
				"type": "string"
			  },
			  "currentUser": {
				"type": "string"
			  },
			  "cpuUsage": {
				"type": "number",
				"minimum": 0,
				"maximum": 100
			  },
			  "cpuCount": {
				"type": "integer",
				"minimum": 1
			  },
			  "publicIp": {
				"type": "string",
				"format": "ipv4"
			  }
			},
			"required": ["devicePlatform", "systemName", "currentUser", "cpuUsage", "cpuCount", "publicIp"]
		  }
		},
		"required": ["device", "system"]
	  }
	},
	"required": ["data"]
  }
  